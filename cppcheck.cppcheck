        <dir name="C:/work/hdrcopy/libhdr-user/"/>
        <dir name="C:/work/hdrcopy/libhdr-arduino/"/>
-->
        <dir name="C:/work/hdrcopy/libhdr-win-arduino-user/"/>
        <dir name="C:/work/hdrcopy/libhdr-win-arduino-std/"/>
        <dir name="C:/work/hdrcopy/libhdr-win-arduino-core/"/>
    </includedir>
    <defines>
        <define name="CONFIG_IDF_TARGET_ESP32"/>
        <define name="CPPCHECK_FAKE"/>
    </defines>
    <suppressions>
        <suppression fileName="*">unusedFunction</suppression>
        <suppression fileName="*">missingIncludeSystem</suppression>
        <suppression fileName="*">missingInclude</suppression>
    </suppressions>
    <check-level-normal/>
</project>
